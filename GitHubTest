package pkgdouble.variables;

/**
 *
 * @author hayou1838
 */
public class DoubleVariables {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        double name1 = 12;
        double name2 = 15;
        int name3 = 15;
        double answer;
        answer = name1 + name2 + name3;
        System.out.println(answer);
    }
    
}
